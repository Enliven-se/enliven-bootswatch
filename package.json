{
  "name": "enliven-bootswatch",
  "description": "Bootstrap bootswatch for ENLIVEN",
  "version": "0.5.0",
  "contributors": [
    "Daniel \"Klokie\" Grossfeld <klokie@enliven.co>",
    "Isaiah Blodgett <isaiah@enliven.co>"
  ],
  "scripts": {
    "setup": "echo 'installing extra dependencies'; npm install git-guppy"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/enliven/enliven-bootswatch.git"
  },
  "private": true,
  "engines": {
    "node": ">=4"
  },
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-register": "^6.5.2",
    "babelify": "^7.3.0",
    "browser-sync": "^2.2.1",
    "browserify": "^13.1.0",
    "del": "^2.2.1",
    "git-guppy": "^2.1.0",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-babel": "^6.1.1",
    "gulp-bump": "^2.7.0",
    "gulp-cache": "^0.4.2",
    "gulp-clean": "^0.3.2",
    "gulp-cssnano": "^2.1.2",
    "gulp-data": "^1.2.1",
    "gulp-environments": "^0.1.1",
    "gulp-eslint": "^2.0.0",
    "gulp-htmlmin": "^2.0.0",
    "gulp-if": "^2.0.1",
    "gulp-imagemin": "^2.2.1",
    "gulp-jade": "^1.1.0",
    "gulp-load-plugins": "^0.10.0",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-rev": "^7.1.0",
    "gulp-rev-replace": "^0.4.3",
    "gulp-sass": "^2.3.2",
    "gulp-size": "^2.1.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-subtree": "^0.1.0",
    "gulp-uglify": "^1.1.0",
    "gulp-useref": "^3.1.0",
    "guppy-cli": "^1.0.1",
    "main-bower-files": "^2.5.0",
    "sc5-styleguide": "^1.1.1",
    "scss-utils": "^1.0.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "wiredep": "^4.0.0"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true,
      "browser": true
    },
    "rules": {
      "quotes": [
        2,
        "single"
      ]
    }
  },
  "dependencies": {
    "bootstrap-sass": "^3.3.7",
    "compass-mixins": "^0.12.10"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "postcss": {
    "plugins": {
      "postcss-easy-import": {
        "extensions": [
          ".css",
          ".scss",
          ".import.css"
        ],
        "prefix": "_"
      },
      "postcss-simple-vars": {},
      "postcss-sassy-mixins": {},
      "postcss-nested": {},
      "autoprefixer": {
        "browsers": [
          "last 2 versions"
        ]
      }
    },
    "parser": "postcss-scss"
  }
}
